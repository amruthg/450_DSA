approach 1 linear time and space

#include<bits/stdc++.h>
using namespace std;

int main(){

    int n;
    cin>>n;

    vector<int> input(n);
    for(int i=0;i<n;i++){

        cin>>input[i];
        if(input[i]!=i+1){
            cout<<"duplicate at i="<<i+1;
            return 0;
        }
    }

    cout<<"no duplicates";
    return 0;
}

